{
  "GET": {
    "summary": "Get the value of a key",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ],
    "since": "0.0.0",
    "group": "string"
  },
  "HGET": {
    "summary": "Get the value of a hash field",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "field",
        "type": "string"
      }
    ],
    "since": "0.0.0",
    "group": "hash"
  },
  "HGETALL": {
    "summary": "Get all the fields and values in a hash",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ],
    "since": "0.0.0",
    "group": "hash"
  },
  "HLEN": {
    "summary": "Get the number of fields in a hash",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ],
    "since": "0.0.0",
    "group": "hash"
  },
  "HMSET": {
    "summary": "Set multiple hash fields to multiple values",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": ["field", "value"],
        "type": ["string", "string"],
        "multiple": true
      }
    ],
    "since": "0.0.0",
    "group": "hash"
  },
  "HSET": {
    "summary": "Set the string value of a hash field",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "field",
        "type": "string"
      },
      {
        "name": "value",
        "type": "string"
      }
    ],
    "since": "0.0.0",
    "group": "hash"
  },
  "LINDEX": {
    "summary": "Get an element index from a list by its value",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "value",
        "type": "string"
      }
    ],
    "since": "0.0.0",
    "group": "indexed-list"
  },
  "LADD": {
    "summary": "Insert an element in a list at the given index",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "index",
        "type": "integer"
      },
      {
        "name": "value",
        "type": "string"
      }
    ],
    "since": "0.0.0",
    "group": "indexed-list"
  },
  "LLEN": {
    "summary": "Get the length of a list",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ],
    "since": "0.0.0",
    "group": "list"
  },
  "SADD": {
    "summary": "Add one or more members to a set",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "member",
        "type": "string",
        "multiple": true
      }
    ],
    "since": "0.0.0",
    "group": "set"
  },
  "SCARD": {
    "summary": "Get the number of members in a set",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ],
    "since": "0.0.0",
    "group": "set"
  },
  "SET": {
    "summary": "Set the string value of a key",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      },
      {
        "name": "value",
        "type": "string"
      },
    ],
    "since": "0.0.0",
    "group": "string"
  },
  "SHUTDOWN": {
    "summary": "Synchronously save the dataset to disk and then shut down the server",
    "arguments": [
      {
        "name": "NOSAVE",
        "type": "enum",
        "enum": ["NOSAVE"],
        "optional": true
      },
      {
        "name": "SAVE",
        "type": "enum",
        "enum": ["SAVE"],
        "optional": true
      }
    ],
    "since": "0.0.0",
    "group": "server"
  },
  "SMEMBERS": {
    "summary": "Get all the members in a set",
    "arguments": [
      {
        "name": "key",
        "type": "key"
      }
    ],
    "since": "0.0.0",
    "group": "set"
  },
}
